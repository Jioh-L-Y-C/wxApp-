page {
  height: 100%;
}
.mine {
  height: 100%;
  background: #f1f3f6;
}
.head-box {
  width: 100%;
  height: 219rpx;
  border-radius: 0 0 50rpx 50rpx;
  background: var(--public_color1);
  padding-top: 30rpx;
}
.myInfo {
  display: flex;
  padding: 0 40rpx;
  justify-content: space-between;
  align-items: center;
}
.myInfo .content {
  display: flex;
}
.myInfo .head-portrait {
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  overflow: hidden;
}
.myInfo .head-portrait image {
  width: 100%;
  height: 100%;
}
.myInfo .mycontent {
  width: 364rpx;
  margin-left: 15rpx;
}
.myInfo .mycontent .nameBox {
  display: flex;
}
.myInfo .mycontent .nameBox .name {
  font-size: 32rpx;
  font-weight: 500;
  color: #ffffff;
}
.myInfo .mycontent .nameBox .level {
  width: 128rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding-left: 44rpx;
  font-size: 18rpx;
  font-weight: 300;
  color: #000000;
  margin-left: 24rpx;
  background: url(data:image/png;base64,);
  background-size: 100%;
}
.myInfo .mycontent .progress {
  margin-top: 5rpx;
}
.myInfo .mycontent .progress .word {
  font-size: 20rpx;
  color: #ffffff;
}
.myInfo .arrow {
  font-size: 16rpx;
  color: #ffffff;
}
.mine-centent-box {
  background: #f1f3f6;
}
.train-box {
  position: relative;
  width: 100%;
  height: 135rpx;
}
.train-list {
  width: 670rpx;
  height: 180rpx;
  display: flex;
  background: #ffffff;
  position: absolute;
  top: -70rpx;
  left: 50%;
  transform: translate(-50%);
  box-shadow: 0px 17px 18px 5px rgba(222, 222, 222, 0.19);
  border-radius: 18px;
}
.train-list .train-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.train-list .train-item .train-item-icon {
  width: 64rpx;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  margin-bottom: 14rpx;
}
.train-list .train-item .blue image {
  width: 26rpx;
  height: 30rpx;
}
.train-list .train-item .green image {
  width: 32rpx;
  height: 32rpx;
}
.train-list .train-item .red image {
  width: 32rpx;
  height: 31rpx;
}
.train-list .train-item .orange image {
  width: 32rpx;
  height: 24rpx;
}
.train-list .train-item .blue {
  background: linear-gradient(-30deg, #258dfc 0%, #53c0ff 100%);
  box-shadow: 0px 20px 15px 1px rgba(41, 146, 252, 0.15);
}
.train-list .train-item .green {
  background: linear-gradient(-30deg, #74b349 0%, #9fe591 100%);
  box-shadow: 0px 20px 15px 1px rgba(120, 184, 79, 0.15);
}
.train-list .train-item .red {
  background: linear-gradient(-30deg, #f81358 0%, #fd7b79 100%);
  box-shadow: 0px 20px 15px 1px rgba(248, 26, 90, 0.15);
}
.train-list .train-item .orange {
  background: linear-gradient(-30deg, #fe7956 0%, #ffa569 100%);
  box-shadow: 0px 20px 15px 1px rgba(254, 126, 88, 0.15);
}
.train-list .train-item .train-word {
  padding-top: 16rpx;
  font-size: 24rpx;
  font-weight: 500;
}
.account-box {
  width: 670rpx;
  height: 310rpx;
  margin: 0 auto;
  background: #ffffff;
  box-shadow: 0px 17px 18px 5px rgba(222, 222, 222, 0.19);
  border-radius: 18px;
}
.feedback-box {
  width: 670rpx;
  height: 390rpx;
  margin: 30rpx auto 0;
  background: #ffffff;
  box-shadow: 0px 17px 18px 5px rgba(222, 222, 222, 0.19);
  border-radius: 18px;
}
.list {
  height: 79rpx;
  line-height: 79rpx;
  border-bottom: 1px solid #FAFAFA;
  margin-left: 40rpx;
  margin-right: 40rpx;
  display: flex;
  justify-content: space-between;
}
.list .list-left {
  display: flex;
}
.list .list-left .list-left-icon {
  font-size: 24rpx;
}
.list .list-left .list-word {
  font-size: 28rpx;
  font-weight: 500;
  padding-left: 24rpx;
}
.list .list-right {
  display: flex;
  align-items: center;
}
.list .list-right .list-right-num {
  font-size: 26rpx;
  color: #FF1C51;
  padding-right: 16rpx;
}
.list .list-right .list-right-gray {
  font-size: 26rpx;
  color: #9CA6B5;
  padding-right: 16rpx;
}
.list .list-right .list-right-icon {
  font-size: 18rpx;
  color: #9CA6B5;
}
